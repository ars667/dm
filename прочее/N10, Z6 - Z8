//N-10
#include <iostream>
#include <cmath>

int main() {
    // Вводим два натуральных числа
    unsigned int a, b;
    std::cout << "Введите первое натуральное число: ";
    std::cin >> a;
    std::cout << "Введите второе натуральное число: ";
    std::cin >> b;
    
    // Вычисляем результат деления
    unsigned int division = a / b;
    
    // Находим первую цифру результата
    unsigned int firstDigit = division;
    while (firstDigit >= 10) {
        firstDigit /= 10;
    }
    
    // Выводим результат
    std::cout << "Первая цифра результата деления, домноженного на 10^k: " << firstDigit * pow(10, floor(log10(division))) << std::endl;
    
    return 0;
}

//Z-6
#include <iostream>

int main() {
    int a, b, sum;
    
    std::cout << "Введите первое число: ";
    std::cin >> a;
    
    std::cout << "Введите второе число: ";
    std::cin >> b;
    
    sum = a + b;
    
    std::cout << "Сумма: " << sum << std::endl;
    
    return 0;
}

//Z-7
#include <iostream>

int main() {
    int a, b, difference;
    
    // Ввод первого числа
    std::cout << "Введите первое число: ";
    std::cin >> a;
    
    // Ввод второго числа
    std::cout << "Введите второе число: ";
    std::cin >> b;
    
    // Вычитание
    difference = a - b;
    
    // Вывод результата
    std::cout << "Разность чисел: " << difference << std::endl;
    
    return 0;
}

//Z-8

#include <iostream>

int main() {
    int a, b, product;

    std::cout << "Введите первое число: ";
    std::cin >> a;

    std::cout << "Введите второе число: ";
    std::cin >> b;

    product = a * b;

    std::cout << "Произведение: " << product << std::endl;

    return 0;
}

